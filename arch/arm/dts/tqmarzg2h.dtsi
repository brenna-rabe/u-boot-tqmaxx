#include "r8a774e1.dtsi"
#include "tqmarzg2-common.dtsi"

/ {
	model = "TQ-Systems TQMaRZG2H on a MBaRZG2X main board";
	compatible = "tq,tqmarzg2h", "renesas,r8a774e1";

	memory@500000000 {
		device_type = "memory";
		reg = <0x5 0x00000000 0x0 0x80000000>;
	};
};

&du {
	clocks = <&cpg CPG_MOD 724>,
		<&cpg CPG_MOD 723>,
		<&cpg CPG_MOD 721>,
		<&du_dotclk_in0>,
		<&du_dotclk_in1>,
		<&du_dotclk_in2>;
		clock-names = "du.0", "du.1", "du.3",
			"dclkin.0", "dclkin.1", "dclkin.2";
};

&sata {
	status = "okay";
};
